#ifndef DOG_H
#define DOG_H
#include <stdio.h>
#include <unistd.h>

/*
 * struct dog - has name, age and owner of a particular dog
 *
 * @name: name of dog
 * @age: age of dog
 * @owner: owner of dog
 *
 * Description: basic details of a dog
 */

struct dog
{
	char *name;
	float age;
	char *owner;
}

/**
 * newtypedog - defining new type for struct dog
 */

typedef struct dog newtypedog;

#endif
